#include <iostream>
#include <thread>
#include <chrono>
using namespace std;

int main(){
    int a, b;

    cout << "First Input:";
    cin >> a;

    cout << "Second Input:";
    cin >> b;

    cout << "SCANNING DATA" << endl;
    this_thread::sleep_for(chrono::seconds(1));
    cout << "First Input:" << b << endl;
    this_thread::sleep_for(chrono::seconds(1));
    cout << "Second Input:" << a << endl;
    


}
